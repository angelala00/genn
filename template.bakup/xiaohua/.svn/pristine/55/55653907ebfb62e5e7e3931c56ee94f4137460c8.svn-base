package com.cjteam.xiao.web.vo;

import java.util.ArrayList;
import java.util.List;

/**
 * Created by ChenLong on 14-2-23.
 */
public class CrapVo extends ResponseVo{
    private UserVo userVo;
    private CrapIssueVo issue;
    private List<String> broadcasts = new ArrayList<String>();
    private String recommend="";// recommend channel code

    public UserVo getUserVo() {
        return userVo;
    }

    public void setUserVo(UserVo userVo) {
        this.userVo = userVo;
    }

    public CrapIssueVo getIssue() {
        return issue;
    }

    public void setIssue(CrapIssueVo issue) {
        this.issue = issue;
    }

    public List<String> getBroadcasts() {
        return broadcasts;
    }

    public void setBroadcasts(List<String> broadcasts) {
        this.broadcasts = broadcasts;
    }

    public String getRecommend() {
        return recommend;
    }

    public void setRecommend(String recommend) {
        this.recommend = recommend;
    }
}
